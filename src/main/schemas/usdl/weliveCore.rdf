<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:ns0="http://purl.org/vocab/vann/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/">
    
    <owl:Ontology rdf:about="http://www.welive.eu/ns/welive-core">
        <dc:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-10-01</dc:created>
        <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-10-01</dc:issued>
        <rdfs:label xml:lang="en">WeLive BB Core</rdfs:label>
        <dc:title xml:lang="en">WeLive BB Core</dc:title>
        <dc:description xml:lang="en">This specification provides the core module of WeLive Building Block metamodel.
            This first draft includes most of the original USDL specification with some useful simplifications. </dc:description>
        <ns0:preferredNamespaceUri>http://www.welive.eu/ns/welive-core#</ns0:preferredNamespaceUri>
        <ns0:preferredNamespacePrefix>welive</ns0:preferredNamespacePrefix>
        <foaf:page rdf:resource="http://www.welive.eu/ns/welive-core.html"/>
        <owl:versionInfo>1.1</owl:versionInfo>
    </owl:Ontology>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#Artifact">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Artifact</rdfs:label>
        <rdfs:comment xml:lang="en">The Artifact class is an abstraction of various building block types. It defines common information both for datasets, services, and public service application.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrService"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#Dataset">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
        <rdfs:label xml:lang="en">Dataset type</rdfs:label>
        <rdfs:comment xml:lang="en">The Dataset class describes the open data set sources as building blocks.</rdfs:comment>
        <rdfs:comment xml:lang="en">Building block as a dataset.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Type"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#BuildingBlock">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">BuildingBlock</rdfs:label>
        <rdfs:comment xml:lang="en">The BuildingBlock class is an abstraction of a generic BB. The class may define the dependency declarations with respect to other BBs or datasets.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#PublicServiceApplication">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">PublicServiceApplication</rdfs:label>
        <rdfs:comment xml:lang="en">The Public Service Application defines a building block that is an application built on top of the other building blocks (services, apps, and/or datasets). The specific characteristics of the application (e.g., execution platform) is defined through the interaction profile.
        </rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#uses">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">BB uses</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates building block dependencies.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
    </rdf:Property>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#type">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">of type</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates building block dependencies.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#Type"/>
    </rdf:Property>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#Type">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Building block type</rdfs:label>
        <rdfs:comment xml:lang="en">This concept covers the kinds of building blocks: WebService, GadgetOpenSocial, MobileApp, Dataset.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#WebService">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Type"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Web service type</rdfs:label>
        <rdfs:comment xml:lang="en">Building block as Web Service.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#GadgetOpenSocial">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Type"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Gadget open social type</rdfs:label>
        <rdfs:comment xml:lang="en">Building block as a gadget Open Social.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#MobileApp">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#Type"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Mobile app type</rdfs:label>
        <rdfs:comment xml:lang="en">Building block as a mobile app.</rdfs:comment>
    </rdf:Description>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#Entity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Entity</rdfs:label>
        <rdfs:comment xml:lang="en">The Entity class is used to characterize the relation between the building blocks and the various stakeholders: owners, providers, authors being physical persons or organizations. This class also represents the role it plays with respect to the BB.</rdfs:comment>
        <rdfs:subClassOf>
            <rdf:Description rdf:about="http://purl.org/goodrelations/v1#BusinessEntity">
                <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/vcard/ns#Kind"/>
            </rdf:Description>
        </rdfs:subClassOf>
        
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#businessRole">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">of role</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates entity business role.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Entity"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#BusinessRole"/>
    </rdf:Property>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#BusinessRole">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Business role.</rdfs:label>
        <rdfs:comment xml:lang="en">Entity role: Provider, Author, Owner.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#Provider">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#BusinessRole"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Provider role</rdfs:label>
        <rdfs:comment xml:lang="en">Provider entity role.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#Owner">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#BusinessRole"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Owner role</rdfs:label>
        <rdfs:comment xml:lang="en">Owner entity role.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#Author">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#BusinessRole"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Author role</rdfs:label>
        <rdfs:comment xml:lang="en">Author entity role.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#hasBusinessRole">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has business role</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates relations between entities oand building blocks.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#Entity"/>
    </rdf:Property>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#TermsConditions">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Terms and Conditions</rdfs:label>
        <rdfs:comment xml:lang="en">The TermConditions class is an abstraction of various restrictions applicable to the building blocks and their properties.</rdfs:comment>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#LegalCondition">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Legal condition</rdfs:label>
        <rdfs:comment xml:lang="en">The legal conditions aim at capturing the aspects of the BB usage related to the licensing, authorship, etc. Specifically, the condition may be represented with the License class.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#TermsConditions"/>
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#hasLegalCondition">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has legal condition</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates legal conditions applicable to the artifact.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#LegalCondition"/>
    </rdf:Property>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#License">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">License</rdfs:label>
        <rdfs:comment xml:lang="en">The software license applied to the artifact. The licenses are devided in Standard Licenses and Custom Licenses.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#LegalCondition"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#StandardLicense">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Standard license</rdfs:label>
        <rdfs:comment xml:lang="en">The Standard Licenses refer to the commonly used license types, such as AST, MIT, GPL, LGPL, and various versions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#License"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#CustomLicense">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Custom license</rdfs:label>
        <rdfs:comment xml:lang="en">The Custom Licenses refer to the application/service specific license agreement.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#License"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#InteractionPoint">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Interaction Point</rdfs:label>
        <rdfs:comment xml:lang="en">This element represents an actual access and invocation of a building block. On a technical level this could translate into calling a Web Service operation, endpoint/format of the dataset, or properties of the service application.</rdfs:comment>
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#hasInteractionPoint">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has interaction point</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the interaction points associated to the artifact.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Artifact"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
    </rdf:Property>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#SOAPWebServiceIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">REST Web service</rdfs:label>
        <rdfs:comment xml:lang="en">The interaction point characterizing a SOAP Web service. The service interface is defined with the corresponding WSDL document</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#RESTWebServiceIP">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">REST Web service</rdfs:label>
        <rdfs:comment xml:lang="en">The interaction point characterizing a REST Web service. The service interface is defined with the corresponding WADL document</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
    </rdfs:Class>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#Parameter">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
        <rdfs:comment xml:lang="en">Characterizes input/output parameters of the interaction.</rdfs:comment>
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#isCollection">
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has interaction point</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates whether the parameter is collection.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Parameter"/>
        <rdfs:range>['true', 'false']</rdfs:range>
    </rdf:Property>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#input">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has input</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the input of the interaction.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#Parameter"/>
    </rdf:Property>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#expectedResult">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has expected result</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the output of the interaction.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#Parameter"/>
    </rdf:Property>
    
    <rdfs:Class rdf:about="http://www.welive.eu/ns/welive-core#TechnicalConstraint">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Technical Constraint</rdfs:label>
        <rdfs:comment xml:lang="en">The technical constraints characterize the interaction point invocation restrictions and characteristics (pre-/post-conditions, monitoring properties).</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#TermsConditions"/>
    </rdfs:Class>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#body">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/description"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">body</rdfs:label>
        <rdfs:comment xml:lang="en">Body of the technical constraint.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal"/>
    </rdf:Property>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#RuleType">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">Technical constraint type.</rdfs:label>
        <rdfs:comment xml:lang="en">Constraint types, such as pre-/post-conditions.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#PRE_CONDITION">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#RuleType"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">pre-condtion</rdfs:label>
        <rdfs:comment xml:lang="en">Pre-condition rule.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#POST_CONDITION">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#RuleType"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">post-condtion</rdfs:label>
        <rdfs:comment xml:lang="en">Post-condition rule.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Description rdf:about="http://www.welive.eu/ns/welive-core#MONITORING">
        <rdfs:subClassOf rdf:resource="http://www.welive.eu/ns/welive-core#RuleType"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">monitoring</rdfs:label>
        <rdfs:comment xml:lang="en">Monitoring rule.</rdfs:comment>
    </rdf:Description>
    
    <rdf:Property rdf:about="http://www.welive.eu/ns/welive-core#hasConstraint">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/subject"/>
        <rdfs:isDefinedBy rdf:resource="http://www.welive.eu/ns/welive-core"/>
        <rdfs:label xml:lang="en">has constraint</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the constraints applied to the interaction.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.welive.eu/ns/welive-core#InteractionPoint"/>
        <rdfs:range rdf:resource="http://www.welive.eu/ns/welive-core#TechnicalConstraint"/>
